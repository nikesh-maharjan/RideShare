@model IEnumerable<RideShareMVC.Models.Tblcontact>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ContactValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactCreateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactUpdateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactTypeNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserGu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ContactValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactCreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactUpdateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactTypeNavigation.ContactTypeGuid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserGu.EmailAddress)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ContactGuid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ContactGuid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ContactGuid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
